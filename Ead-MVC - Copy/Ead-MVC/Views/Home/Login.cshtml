}

<section class="title">
    <div class="login-head">
        <div class="container">
            <div class="row">
                <div class="listing-head">
                    <h1 class="page-title">Login</h1>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="container">
    <div class="row form-title">
        <h2><span>Login</span></h2>
    </div>
    <div class="col-md-8 col-md-offset-2">
        <div class="">

            <div class="">
                <form class="form-horizontal" method="POST" action="https://www.wingding.pk/login" style="max-width: 630px;">
                    <input type="hidden" name="_token" value="JCRDJgZXhgjwgEJSwDORI11TkOzaApRIQcHRFoh3">



                    <div class="form-group">
                        <label for="email" class="col-md-4 control-label">E-Mail Address</label>

                        <div class="col-md-6">
                            <input id="email" type="email" class="form-control" name="email" value="" required autofocus>
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="password" class="col-md-4 control-label">Password</label>
                        <div class="col-md-6">
                            <input id="password" type="password" class="form-control" name="password" required>

                            <button type="submit" class="btn btn-primary" style="float:left;width: 120px;margin-top: 8px;">
