                            </div>
                            <div class="client-face">
                                <img src=@Url.Content("/Content/frontend/assets/img/client-face1.png") alt="Client">
                                <h6 class="pink">Shahid Ali Rajput</h6>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

