@{
    ViewBag.Title = "Home Page";
}

<div class="slide-2">
    <div id="slider" class="sl-slider-wrapper">
        <div class="sl-slider">
            <div class="sl-slide" data-orientation="horizontal" data-slice1-rotation="-25" data-slice2-rotation="-25" data-slice1-scale="2" data-slice2-scale="2">
                <div class="sl-slide-inner ">

                    <div class="bg-img bg-img-1" style="background-image: url(/Content/frontend/assets/img/Slider4.jpg);"></div>
                    <blockquote>
                        <article>Your Celebrations <span class="hidden-xs">|</span> Our Calibrations</article>
                    </blockquote>
                </div>
            </div>

            <div class="sl-slide" data-orientation="vertical" data-slice1-rotation="10" data-slice2-rotation="-15" data-slice1-scale="1.5" data-slice2-scale="1.5">

                <div class="sl-slide-inner ">

                    <div class="bg-img bg-img-1" style="background-image: url(/Content/frontend/assets/img/Slider2.jpg);"></div>
                    <blockquote>
                        <cite>Seek, Find & Book</cite>
                        <article>Revolutionizing Celebrations With WingDing</article>
                    </blockquote>
                </div>
            </div>

            <div class="sl-slide" data-orientation="horizontal" data-slice1-rotation="3" data-slice2-rotation="3" data-slice1-scale="2" data-slice2-scale="1">

                <div class="sl-slide-inner ">

                    <div class="bg-img bg-img-1" style="background-image: url(/Content/frontend/assets/img/Slider3.jpg);"></div>
                    <blockquote>
                        <cite>Your Events Managed</cite>
                        <article>Your Own Way</article>
                    </blockquote>
                </div>
            </div>


            <div class="sl-slide" data-orientation="horizontal" data-slice1-rotation="-5" data-slice2-rotation="10" data-slice1-scale="2" data-slice2-scale="1">

                <div class="sl-slide-inner ">

                    <div class="bg-img bg-img-1" style="background-image: url(/Content/frontend/assets/img/Slider.jpg);"></div>

                    <blockquote>
                        <cite>Locate & Curate From Thousand Vendors</cite>
                        <article>Anytime, Any place!</article>

                    </blockquote>
                </div>
            </div>
        </div><!-- /sl-slider -->

        <nav id="nav-dots" class="nav-dots">
            <span class="nav-dot-current"></span>
            <span></span>
            <span></span>
            <span></span>
        </nav>
    </div><!-- /slider-wrapper -->
</div>
<!--End of slider-->
