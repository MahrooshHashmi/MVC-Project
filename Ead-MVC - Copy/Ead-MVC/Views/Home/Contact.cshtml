@{
    ViewBag.Title = "Contact";
}
<section class="title">
    <div class="contact-head">
        <div class="container">
            <div class="row">
                <div class="listing-head">
                    <h1 class="page-title">Contact Us</h1>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="contact-form">
    <div class="container">
        <div class="row form-title">
            <h2><span>Contact Us</span></h2>
        </div>
        <form method="POST" action="https://www.wingding.pk/contact/submit">

            <input type="hidden" name="_token" value="JCRDJgZXhgjwgEJSwDORI11TkOzaApRIQcHRFoh3">

            <!--  yeh field har form main lagani ha -->
            <div class="row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <input type="text" required class="form-control" id="name" name="name" placeholder="Your Name">
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <input type="email" required class="form-control" id="email" name="email" placeholder="Your Email ID">
                    </div>
                </div>

                <div class="col-sm-12">
                    <div class="form-group">
                        <input type="text" required class="form-control" id="subject" name="subject" placeholder="Your Subject">
                    </div>
                </div>
                <div class="col-sm-12">
                    <div class="form-group">
                        <textarea id="message" required class="form-control" name="message" placeholder="Your Message"></textarea>
                    </div>
                </div>
                <div class="col-sm-12 text-center">
                    <button type="submit" class="btn btn-primary">Submit Message</button>
                </div>
            </div>
            <!-- /.row -->
        </form>
    </div>
</section>
<section class="contact-div">
    <div class="container">
        <div class="row contact-info">
            <div class="col-md-4  col-sm-4 contact-icon" style="border-right: 1px solid #dcd5cd;">
                <div class="col-md-4">

                    <img src="~/Content/frontend/assets/img/address-icon.png" alt="Address">

                </div>
                <div class="col-md-8">
                    <ul>
                        <li>Address</li>
                        <li>33 K, Jaffer Street, PECHS,Block 6, Karachi</li>

                    </ul>
                </div>

            </div>
            <div class="col-md-4  col-sm-4 contact-icon" style="border-right: 1px solid #dcd5cd;">
                <div class="col-md-4">
                    <img alt="Call" src="~/Content/frontend/assets/img/phone-icon.png">
                </div>
                <div class="col-md-8">
                    <ul>
                        <li>Contact No</li>
                        <li>030-wingding (94643464)</li>

                    </ul>
                </div>
            </div>
            <div class="col-md-4  col-sm-4 contact-icon">
                <div class="col-md-4">
                    <img alt="Email" src="~/Content/frontend/assets/img/email-icon.png">
                </div>
                <div class="col-md-8">
                    <ul>
                        <li>Email Address</li>
                        <li>info@wingding.pk</li>
                    </ul>
                </div>

            </div>
        </div>
    </div>
</section>
<section class="map-div">
    <div class="container-fluid">
        <div class="row">
            <!--     <img alt="" src="rontend/assets/img/maps-img.jpg">-->

            <div style="width: 100%; margin-top: 50px;">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d904.9826409935952!2d67.07200982919521!3d24.866220999003186!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3eb33ea2e62c35cb%3A0x8b6f14b443648b66!2sWingDing!5e0!3m2!1sen!2s!4v1523619547563" width="100%" height="400" frameborder="0" style="border:0"></iframe>
            </div>
        </div>
    </div>
</section>